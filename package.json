{
  "name": "Aquamind",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios --simulator=\"iPhone 12 Pro\"",
    "start": "npx react-native start",
    "pod": "npx pod-install",
    "podUpdate": "npx pod-install --repo-update",
    "images": "npx expo-optimize",
    "clean": "cd android && ./gradlew cleanBuildCache && ./gradlew clean",
    "reset": "yarn clean && rm -f ./yarn.lock && rm -rf ./node_modules && rm -f ./ios/Podfile.lock.lock && rm -rf ./ios/Pods && yarn install && yarn pod",
    "resetMetro": "watchman watch-del-all && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-*",
    "android:release": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk/release",
    "jsbundle": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "server": "json-server -p 3333 -d 500 --watch ./src/services/server.json"
  },
  "dependencies": {
    "@react-native-community/art": "^1.2.0",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/toolbar-android": "^0.1.0-rc.2",
    "@react-navigation/material-bottom-tabs": "^5.2.14",
    "@react-navigation/native": "^5.7.1",
    "@react-navigation/stack": "^5.7.1",
    "@reduxjs/toolkit": "^1.4.0",
    "axios": "^0.19.2",
    "color": "^3.1.2",
    "expo-notifications": "^0.4.0",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-animatable": "^1.3.3",
    "react-native-fast-image": "^8.3.2",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-image-picker": "^2.3.3",
    "react-native-paper": "^4.0.1",
    "react-native-progress": "^4.1.2",
    "react-native-reanimated": "^1.10.1",
    "react-native-redash": "^14.2.3",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.9.0",
    "react-native-swipe-list-view": "^3.2.3",
    "react-native-unimodules": "^0.10.1",
    "react-native-vector-icons": "^7.0.0",
    "react-navigation-heavy-screen": "^1.0.2",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.1.1",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/color": "^3.0.1",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.157",
    "@types/react-native": "^0.62.0",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^5.1.1",
    "@types/yup": "^0.29.3",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "expo": "^38.0.8",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.4",
    "react-test-renderer": "16.13.1",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "typescript": "^3.8.3"
  }
}
